{
	"name": "{{appDescription}}",
    "prefix": "kyper.data",
	"members": [
    {{#operations}}
        {{#operation}} {
    	"name":"{{nickname}}",
    	"description":"{{notes}}",
    	"params": [{{#allParams}} 
			{"name":"{{paramName}}", "type": ["{{dataType}}"], "default":"{{#defaultValue}}{{/defaultValue}}" , "description":"{{description}} {{^optional}}(required){{/optional}}{{#optional}}(optional){{/optional}}" }, {{newline}} {{/allParams}}
		],
    	"class_method": true,
    	"ret": {"type":"{{returnType}}", "description":"{{summary}}"},
        },{{/operation}}
    {{/operations}}
	]
}

